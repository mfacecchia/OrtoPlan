<dialog id = "newPlantPlantation" class = "modal" onclose = "resetModalForm(this)">
    <div class = "modal-box">
        <form method = "dialog">
            <button class = "btn btn-sm btn-square btn-outline btn-primary rounded-md absolute right-2 top-2" tabindex = "0">X</button>
        </form>
        <form id = "newPlantPlantationForm">
            <header>
                <% if(typeof customHeader !== 'undefined'){ %>
                    <b><%= customHeader %></b>
                <% }else{ %>
                    <b>New element</b>
                <% } %>
                <% if(typeof customTitle !== 'undefined'){ %>
                    <h1><%= customTitle %></h1>
                <% }else{ %>
                    <h1>Create a new element</h1>
                <% } %>
            </header>
            <% formFields.forEach(field => { %>
                <%- field %>
            <% }) %>
        </form>
    </div>
</dialog>